{
  "name": "ecorp-ctf-challenge",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:bot": "nodemon --watch bot.ts --exec ts-node bot.ts ",
    "setup": "docker compose -f docker-compose.base.yaml build && docker compose -f docker-compose.base.yaml up",
    "db-bootstrap": "yarn prisma generate && yarn prisma migrate dev --name init && yarn prisma db seed --preview-feature",
    "up-dev": "docker compose -f docker-compose.base.yaml up",
    "down-dev": "docker compose -f docker-compose.base.yaml down",
    "build-dev": "docker compose -f docker-compose.base.yaml build",
    "up-prod": "docker compose -f docker-compose.prod.yaml up",
    "down-prod": "docker compose -f docker-compose.prod.yaml down",
    "build-prod": "docker compose -f docker-compose.prod.yaml build",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "seed-db": "ts-node prisma/seed.ts",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@apollo/server": "^4.7.1",
    "@as-integrations/next": "^1.3.0",
    "@pothos/core": "^3.30.0",
    "@pothos/plugin-prisma": "^3.51.1",
    "@prisma/client": "^4.14.0",
    "@tanstack/react-query": "^4.29.7",
    "@types/node": "20.1.3",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "apollo-server-express": "^3.12.0",
    "autoprefixer": "10.4.14",
    "bcrypt": "^5.1.0",
    "cookie": "^0.5.0",
    "dotenv": "^16.3.1",
    "encoding": "^0.1.13",
    "eslint": "8.40.0",
    "eslint-config-next": "13.4.2",
    "graphql": "^16.6.0",
    "graphql-request": "^6.0.0",
    "graphql-yoga": "^3.9.1",
    "jsdom": "^22.1.0",
    "jsonwebtoken": "^9.0.0",
    "next": "13.4.2",
    "postcss": "8.4.23",
    "prisma": "^4.14.0",
    "puppeteer": "^13.5.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "request": "^2.88.2",
    "tailwindcss": "3.3.2",
    "ts-node": "^10.9.1",
    "typescript": "5.0.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.1",
    "@graphql-codegen/cli": "4.0.0",
    "@graphql-codegen/client-preset": "4.0.0",
    "@graphql-codegen/introspection": "4.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie": "^0.5.1",
    "@types/jsdom": "^21.1.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/uuid": "^9.0.1",
    "nodemon": "^3.0.1"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "peerDependencies": {
    "canvas": "^2.11.2"
  }
}
