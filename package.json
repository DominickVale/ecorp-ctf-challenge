    "dotenv": "^16.3.1",
    "encoding": "^0.1.13",
    "graphql": "^16.6.0",
    "graphql-request": "^6.1.0",
    "gray-matter": "^4.0.3",
    "jsdom": "^22.1.0",
    "jsonwebtoken": "^9.0.0",
    "next": "13.4.2",
    "postcss": "8.4.23",
